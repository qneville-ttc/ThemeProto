<script type="text/javascript" src="https://apply.resumemirror.com/js/jquery.simplemodal-1.3.5.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        {% if confirmUrl %}
          url = "{{confirmUrl}}?redirect_url={{return_url}}";
        {% else %}
          url = "{{return_url}}";
        {% endif %}
        
        window.parent.closeModal(url);      
    });
</script>