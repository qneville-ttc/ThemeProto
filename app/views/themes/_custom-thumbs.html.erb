<!-- This is a theme thumbnail button used on index.html.erb -->
<% @themes_custom.each do |theme| %>


	<div class="theme_thumb" thm="<%= theme.id %>" >
	  <div class="thumbnail" style="background-image: url()">
	    <div class="thumb_tooltip"><small><strong><%= theme.theme_name %>:</strong><br/><%= theme.theme_desc %></small>
	      <div class="options">
	      	<%= image_tag("edit.png", :border => 0, :class => "edit_theme", :id_rel => "#{theme.id}", :alt => "Edit Theme", :title => "Edit Theme Contents" ) %>
	      	<%= image_tag("duplicate.png", :border => 0, :class => "duplicate_theme", :id_rel => "#{theme.id}", :alt => "Clone Theme", :title => "Clone Theme" ) %>
	      	<%= image_tag("trash.png", :border => 0, :class => "trash_theme", :id_rel => "#{theme.id}", :alt => "Duplicate Theme", :title => "Delete Theme") %>
	      </div>
	    </div>
	    <img class="loader" src="/images/site_assets/theme-previews/ajax-load-32px.gif"/>
	  </div>
	  <a class="" id="">Set Theme</a>
	</div>

<% end %>


	<div class="addNew" thm="" >
	  <a class="add" id=""></a>
	</div>